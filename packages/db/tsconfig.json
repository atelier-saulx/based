{
  "extends": "@saulx/tsconfig/default.json",
  "compilerOptions": {
    "outDir": "dist",
    "esModuleInterop": true,
    "allowJs": true,
    "noPropertyAccessFromIndexSignature": false,
    "rootDir": "./",
    "composite": true,
    "moduleResolution": "NodeNext",
    "module": "NodeNext",
    "target": "esnext",
    "noEmit": true,
    "allowImportingTsExtensions": true
  },
  "include": ["./src", "./test"],
  "exclude": ["./dist"]
}
