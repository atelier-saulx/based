{
  "name": "@based/graphql",
  "version": "2.3.5",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "buildBundle": "node ./build/index.js",
    "build": "npx tsc && npm run buildBundle",
    "watch": "npm run build -- --watch",
    "clean": "rimraf {.turbo,dist,node_modules}"
  },
  "sideEffects": false,
  "dependencies": {
    "@based/types": "2.3.5",
    "graphql": "^16.3.0"
  },
  "ava": {
    "files": [
      "test/*.ts"
    ],
    "concurrency": 1,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "devDependencies": {
    "esbuild": "^0.14.29",
    "ava": "3.15.0",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5",
    "rimraf": "^3.0.2"
  }
}
