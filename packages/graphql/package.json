{
  "name": "@based/graphql",
  "version": "3.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "buildBundle": "node ./build/index.js",
    "build": "npx tsc",
    "watch": "npm run build -- --watch",
    "clean": "rimraf {.turbo,dist,node_modules}",
    "test": "ava --color --node-arguments='--expose-gc' --timeout 3m --verbose"
  },
  "sideEffects": false,
  "dependencies": {
    "graphql": "^16.3.0"
  },
  "ava": {
    "files": [
      "test/*.ts"
    ],
    "concurrency": 1,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "devDependencies": {
    "@saulx/selva-server": "21.1.0",
    "@saulx/selva": "21.1.0",
    "esbuild": "^0.14.29",
    "ava": "5.3.1",
    "ts-node": "10.9.1",
    "typescript": "^4.3.5",
    "rimraf": "^3.0.2"
  }
}
