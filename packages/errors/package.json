{
  "name": "@based/errors",
  "version": "1.3.1-alpha.1",
  "license": "MIT",
  "type": "module",
  "main": "./dist/src/index.js",
  "exports": {
    "./client": "./dist/src/client.js",
    ".": {
      "node": {
        "require": "./dist/index.cjs"
      },
      "default": "./dist/src/index.js"
    }
  },
  "scripts": {
    "buildCjs": "npx esbuild ./src/index.ts --bundle --platform=node --minify --outfile=./dist/index.cjs",
    "build": "npx tsc && cp ./dist/src/index.d.ts ./dist/src/index_original.d.ts && npm run buildCjs",
    "watch": "npx tsc --watch",
    "test": "npx ava",
    "clean": "rm -rf ./dist ./node_modules"
  },
  "sideEffects": false,
  "ava": {
    "timeout": "2m",
    "workerThreads": false,
    "files": [
      "./dist/test/**/*.js"
    ]
  },
  "files": [
    "dist/src",
    "dist/index.cjs",
    "README.md",
    "package.json"
  ],
  "dependencies": {
    "@based/functions": "3.3.2-alpha.1"
  },
  "devDependencies": {
    "ts-node": "10.9.1",
    "ava": "5.3.1",
    "typescript": "^4.3.5"
  }
}
