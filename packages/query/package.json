{
  "name": "@based/db-query",
  "version": "2.0.0-alpha.26",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx tsc",
    "watch": "npm run build -- --watch",
    "clean": "rimraf {.turbo,dist,node_modules}",
    "test": "ava --color --node-arguments='--expose-gc' --timeout 3m --verbose"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "!dist/**/*.js.map"
  ],
  "ava": {
    "files": [
      "test/*.ts"
    ],
    "concurrency": 1,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "dependencies": {
    "@saulx/diff": "^1.1.4",
    "@saulx/utils": "^3.2.2",
    "@based/client": "^4.7.6"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "ava": "3.15.0",
    "ts-node": "10.9.1",
    "typescript": "^5.1.6",
    "rimraf": "^3.0.2"
  }
}
