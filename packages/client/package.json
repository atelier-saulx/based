{
  "name": "@based/client",
  "version": "4.8.10",
  "license": "MIT",
  "scripts": {
    "build": "npx tsc && cp ./dist/src/index.d.ts ./dist/src/index_original.d.ts",
    "test-stream": "npx ts-node ./test/browser/upload-file.ts",
    "watch": "npx tsc --watch",
    "clean": "rimraf {.turbo,dist,node_modules}",
    "test": "ava --verbose",
    "browserBuild": "esbuild ./test/browser/index.ts --bundle --outfile=./test/browser/out.js --minify --metafile=./test/browser/meta.json",
    "browser": "concurrently \"nodemon ./test/browser/based-server.ts --watch ../server/dist --watch ./test/browser/based-server.ts\" \"aristotle watch -t test/browser/empty.ts -p 8006\""
  },
  "type": "module",
  "main": "./dist/src/index.js",
  "ava": {
    "concurrency": 1,
    "timeout": "2m",
    "files": [
      "./dist/test/**/*.js"
    ]
  },
  "files": [
    "dist/src",
    "README.md",
    "package.json"
  ],
  "dependencies": {
    "@saulx/diff": "^2.0.1",
    "@saulx/hash": "^3.0.0",
    "@saulx/utils": "^4.0.1",
    "@based/fetch": "^1.0.3",
    "@based/opts": "0.7.4",
    "fflate": "0.8.1",
    "isomorphic-ws": "^5.0.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "esbuild": "^0.19.5",
    "@aws-sdk/lib-storage": "^3.304.0",
    "@aws-sdk/client-s3": "^3.304.0",
    "nodemon": "^2.0.20",
    "@based/functions": "^2.0.1",
    "concurrently": "^7.1.0",
    "@saulx/aristotle": "^4.3.0",
    "ava": "5.3.1",
    "@saulx/tsconfig": "^1.0.5",
    "@saulx/prettier-config": "^1.0.0",
    "typescript": "^5.2.2",
    "rimraf": "^3.0.2"
  }
}
