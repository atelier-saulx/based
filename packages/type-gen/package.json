{
  "name": "@based/type-gen",
  "version": "0.0.1",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "test": "ava --color --node-arguments='--expose-gc' --timeout 3m --verbose",
    "build": "npx tsc",
    "watch": "npx tsc --watch",
    "clean": "rimraf {.turbo,dist,node_modules}"
  },
  "sideEffects": false,
  "dependencies": {
    "utility-types": "^3.10.0",
    "@saulx/utils": "^3.5.0",
    "@based/functions": "^2.2.3"
  },
  "devDependencies": {
    "ts-node": "10.9.1",
    "typescript": "^4.3.5",
    "fs-extra": "^11.1.1",
    "rimraf": "^3.0.2",
    "ava": "3.15.0"
  },
  "ava": {
    "files": [
      "test/*.ts"
    ],
    "concurrency": 1,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
