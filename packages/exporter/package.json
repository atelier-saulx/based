{
  "name": "@based/exporter",
  "version": "0.1.3",
  "main": "dist/exporter.js",
  "scripts": {
    "clean": "rm -Rf node_modules dist",
    "test": "tsc -b && node dist/exporter.js",
    "test-watch": "tsc --watch & node --watch dist/exporter.js",
    "watch": "tsc -b --watch",
    "build": "tsc -b",
    "start": "node dist/exporter.js"
