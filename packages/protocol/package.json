{
  "name": "@based/protocol",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "npx tsc",
    "watch": "npm run build -- --watch",
    "test": "ava --timeout 1m --verbose",
    "watchTest": "ava --color --watch --verbose",
    "clean": "rimraf {.turbo,dist,node_modules}"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "src/index.ts",
  "dependencies": {
    "json-joy": "^1.18.1",
    "jsonpack": "^1.1.5",
    "brotli": "^1.3.3",
    "@based/types": "^2.6.0"
  },
  "ava": {
    "files": [
      "test/*.ts"
    ],
    "concurrency": 1,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5",
    "ava": "3.15.0",
    "rimraf": "^3.0.2"
  }
}
