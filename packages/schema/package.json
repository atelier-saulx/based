{
  "name": "@based/schema",
  "version": "1.0.6",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx tsc",
    "watch": "npx tsc --watch",
    "clean": "rimraf {.turbo,dist,node_modules}",
    "test": "ava --color --node-arguments='--expose-gc' --timeout 3m --verbose"
  },
  "sideEffects": false,
  "dependencies": {
    "validator": "^13.9.0",
    "@saulx/hash": "^2.0.0",
    "@saulx/utils": "^3.5.0"
  },
  "ava": {
    "files": [
      "test/*.ts"
    ],
    "concurrency": 1,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "devDependencies": {
    "type-fest": "^3.12.0",
    "ts-node": "10.9.1",
    "typescript": "^4.3.5",
    "rimraf": "^3.0.2",
    "ava": "3.15.0"
  }
}
